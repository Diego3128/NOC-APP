generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

enum LogSeverity {
  LOW
  MEDIUM
  HIGH
}

model Log {
  id      String      @id @default(uuid())
  message String
  origin  String
  date    DateTime    @default(now())
  level   LogSeverity @default(LOW)
}
